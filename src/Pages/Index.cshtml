@page
@inject IConfiguration _config
@inject AppDbContext _db
@model IndexModel
@{
    var PostPartial = "Themes/" + _config["Theme"] + "/_Post";
    ViewData["Title"] = "Home page";
}

@{ 
    foreach(Post p in Model.Posts)
    {
        <partial name="@PostPartial" model="@p" />
    }
}

